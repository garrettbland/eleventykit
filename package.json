{
    "name": "eleventykit",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "prettier": {
        "semi": false,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 70,
        "tabWidth": 4,
        "useTabs": false,
        "bracketSpacing": true
    },
    "scripts": {
        "clean": "rimraf dist",
        "serve:eleventy": "npx @11ty/eleventy --serve",
        "serve:parcel": "cross-env NODE_ENV=development parcel watch ./src/js/main.js --out-dir dist/assets",
        "dev": "npm-run-all clean --parallel serve:*",
        "build:eleventy": "npx @11ty/eleventy",
        "build:parcel": "cross-env NODE_ENV=production parcel build ./src/js/main.js --out-dir dist/assets",
        "build": "npm-run-all clean build:*"
    },
    "keywords": [
        "eleventy",
        "alpinejs",
        "tailwindcss",
        "static",
        "parcel"
    ],
    "author": "Garrett Bland",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^0.11.1",
        "@babel/core": "^7.12.13",
        "@babel/preset-env": "^7.12.13",
        "@tailwindcss/postcss7-compat": "^2.0.3",
        "@tailwindcss/typography": "^0.4.0",
        "alpinejs": "^2.8.0",
        "autoprefixer": "^9.8.6",
        "cross-env": "^7.0.3",
        "feather-icons": "^4.28.0",
        "html-minifier": "^4.0.0",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "postcss": "^7.0.35",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
    },
    "devDependencies": {}
}
